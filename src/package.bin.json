{
  "main": "index.html",
  "name": "Haroopad",
  "description": "The Next Document processor based on Markdown",
  "version": "0.13.0",
  "keywords": [
    "markdown",
    "haroopress",
    "editor"
  ],
  "window": {
    "icon": "logo.png",
    "toolbar": false,
    "show": false,
    "width": 0,
    "height": 0,
    "min_width": 700,
    "min_height": 350,
    "frame": false
  },
  "single-instance": true,
  "webkit": {
    "plugin": true
  },
  "chromium-args": "--enable-threaded-compositing",
  "app": {
    "upgrade": "http://pad.haroopress.com/upgrade.json"
  },
  "maintainers": [{
    "name": "Rhio Kim",
    "email": "rhio.kim@gmail.com",
    "web": "http://pad.haroopress.com",
    "blog": "http://pad.haroopress.com/page.html",
    "feedback": "http://haroopad.userecho.com"
  }],
  "licenses": [{
    "type": "GPLv2",
    "url": "http://www.example.com/licenses/gpl.html"
  }],
  "bugs": "https://github.com/rhiokim/haroopad/issues",
  "repositories": [{
    "type": "git",
    "url": "git@github.com:rhiokim/haroopad.git"
  }],
  "dependencies": {}
}